
ï»¿using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace AlgorithmPractice
{
    internal class ClimbStairs
    {
        public int ClimbStairs(int n) {
            if (n == 0){
              return 1;
            }
            if(n < 0 ){
              return 0;
            }
            return ClimbStairs(n-1)+ ClimbStairs(n-2);
        }
    }
}
